pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
-- main
-- - trocar posicao
-- - mover caixa
-- - animacao entre quadros
-- - identificar sucesso
-- - desenho porta
-- - mudar porta no sucesso
-- - abertura
function _init()
 level=1
 p={
 	x=0,
 	y=0
 }
 start=find_start(0,0)
 p.x=start[1]
 p.y=start[2]
 clear_start(start)
end

function _update()
 local x=p.x
 local y=p.y
 local step=16
 if btnp(⬅️) then
  x-=step
 end
 if btnp(➡️) then
  x+=step
 end
 if btnp(⬆️) then
  y-=step
 end
 if btnp(⬇️) then
  y+=step
 end
 if fget(mget(x/8,y/8))==1 then
  x=p.x
  y=p.y
 end
 p.x=x
 p.y=y

 camera(p.x\128*128,p.y\128*128)
end

function _draw()
 cls()
 map()
 
 color(3)
 spr(66,p.x,p.y,2,2)
 --debug_map()
end
-->8
-- debug
function debug_map()
 r={1,2,3,4,5,6,7,8}
 for i in all(r) do
 for j in all(r) do
 	--print(""..i..","..j, i*14,j*6)
  pi=i
  pj=j
  px=i*14
  py=j*6
  mg=mget(i,j)
  fg=fget(mg)
  print(mg,px,py)
 end
 end
end
-->8
-- helpers

function clear_start(start)
 local x=start[1]/8
 local y=start[2]/8
 mset(x,y,0)
 mset(x+1,y,0)
 mset(x,y+1,0)
 mset(x+1,y+1,0)
end

function find_start(map_x, map_y)  map_w=16
 map_h=16
 player_marker=64
 for y=0,map_h-1 do
  for x=0,map_w-1 do
   local tile = mget(map_x + x, map_y + y)
   if tile == player_marker then
    return {(map_x + x) * 8, 
            (map_y + y) * 8}
   end
  end
 end
 return nil, nil
end

