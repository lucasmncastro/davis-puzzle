pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
-- main
-- x trocar posicao
-- x mover caixa
-- - animacao entre quadros
-- - identificar sucesso
-- - desenho porta
-- - mudar porta no sucesso
-- - abertura

palt(0,false)
palt(1,true)

function _init()
 p={
 	x=0,
 	y=0,
 	p=3,
 	s=66
 }
 init_objs()
 
 level=1
 init_level()
end

function init_objs()
 objs={}
 for y=0,128,2 do
  for x=0,128,2 do
   local s=mget(x,y)
   if fget(s)==8 then
    add(objs,{x=x,y=y,s=s})
    mset(x,y)
    mset(x+1,y)
    mset(x,y+1)
    mset(x+1,y+1)
   end
  end
 end
end

function init_level()
 start=find_start(16,0)
 p.x=start[1]
 p.y=start[2]
 clear_start(start)
end

function _update()
 local x=p.x
 local y=p.y
 local step=16
 local dir
 if btnp(⬅️) then
  dir=⬅️
  x-=step
  p.p=3
 end
 if btnp(➡️) then
  dir=➡️
  x+=step
  p.p=1
 end
 if btnp(⬆️) then
  dir=⬆️
  y-=step
 end
 if btnp(⬇️) then
  dir=⬇️
  y+=step
 end
 local flag=fget(mget(x/8,y/8))
 if flag==1 then
  x=p.x
  y=p.y
 end

 -- move object
 for o in all(objs) do
  local hit=(
   o.x==x\8 and o.y==y\8
  )
  function is_free(x,y)
   local nspr=mget(x\8,y\8)
   local nflg=fget(nspr)
   local block=nflg==1
   if (block) return false   
   for o in all(objs) do
    if o.x==x\8 and o.y==y\8 then
     return false
    end
   end
   return true
  end
  if hit then
   if dir==➡️ then
    if is_free(x+16,y) then
	    o.x+=2
   	else	
   	 x=p.x
	    y=p.y
   	end
   end
   if dir==⬇️ then
    if is_free(x,y+16) then
	    o.y+=2
   	else
	    x=p.x
	    y=p.y
   	end
   end
   if dir==⬅️ then
    if is_free(x-16,y) then
	    o.x-=2
   	else
	    x=p.x
	    y=p.y
   	end
   end
   if dir==⬆️ then
    if is_free(x,y-16) then
	    o.y-=2
   	else
	    x=p.x
	    y=p.y
   	end
   end

  end
 end

 p.x=x
 p.y=y

 camera(p.x\128*128,p.y\128*128)
end

function _draw()
 cls()
 map()
 
 for o in all(objs) do
  spr(o.s,o.x*8,o.y*8,2,2)
 end
 
 spr(p.s,p.x,p.y,2,2,(p.p==1))

 --debug_p()
 --debug_map() 
 --debug_objs()
end
-->8
-- debug
function debug_map()
 r={1,2,3,4,5,6,7,8}
 for i in all(r) do
 for j in all(r) do
 	--print(""..i..","..j, i*14,j*6)
  pi=i
  pj=j
  px=i*14
  py=j*6
  mg=mget(i,j)
  fg=fget(mg)
  print(mg,px,py)
 end
 end
end

function debug_objs()
 color(8)
 print("debug:",p.x,p.y+16)
 for o in all(objs) do
  print(o.x..","..o.y)
  print(mget(o.x-1,o.y))
 end
 print(#objs)
end

function debug_p()
 color(3)
 print("debug:",p.x,p.y+16)
 print(""..p.x..","..p.y)
 print("mget:"..mget(p.x\8,p.y\8))
 print("fget:"..fget(mget(p.x\8,p.y\8)))
end
-->8
-- helpers

function clear_start(start)
 local x=start[1]/8
 local y=start[2]/8
 mset(x,y,0)
 mset(x+1,y,0)
 mset(x,y+1,0)
 mset(x+1,y+1,0)
end

function find_start(map_x, map_y)
 map_w=16
 map_h=16
 player_marker=64
 for y=0,map_h-1 do
  for x=0,map_w-1 do
   local tile = mget(map_x + x, map_y + y)
   if tile == player_marker then
    return {(map_x + x) * 8, 
            (map_y + y) * 8}
   end
  end
 end
 return nil, nil
end

